{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "UseInMemoryDatabase": "true",
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Initial Catalog=dotnet5-petclinic-rest;Integrated Security=true;MultipleActiveResultSets=True"
  },
  "Swashbuckle": {
    "SwaggerGen": {
      "SwaggerGeneratorOptions": {
        "SwaggerDocs": {
          "v1": {
            "Title": "Pet Clinic Rest (.NET 5) API",
            "Version": "v1"
          }
        }
      }
    },
    "Swagger": {
      "RouteTemplate": "swagger/{documentName}/swagger.json"
    },
    "SwaggerUI": {
      "RoutePrefix": "swagger",
      "ConfigObject": {
        "Urls": [
          {
            "Url": "/swagger/v1/swagger.json",
            "Name": "Pet Clinic Rest (.NET 5) API V1"
          }
        ],
        "DeepLinking": true,
        "DisplayOperationId": true,
        "DefaultModelsExpandDepth": -1,
        "DefaultModelExpandDepth": 2,
        "DefaultModelRendering": "model",
        "DocExpansion": "list",
        "Filter": "",
        "ShowExtensions": true
      }
    }
  }
}